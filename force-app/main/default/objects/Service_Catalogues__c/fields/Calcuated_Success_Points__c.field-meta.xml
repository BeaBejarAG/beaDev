<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Calcuated_Success_Points__c</fullName>
    <description>Calculates the Success Points based on the monetary value</description>
    <externalId>false</externalId>
    <formula>CEILING((Service_Cost__c * 0.63)/10)*10</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Calculated Success Points</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
