    <formulas>
        <description>Moves Accounts to rows</description>
        <name>AccountsToRows</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE({!Select_Accounts_To_Share_With},&quot;;&quot;,&quot;&lt;br/&gt;&quot;)</expression>
    </formulas>
    <formulas>
        <description>Split Entitlements to Rows</description>
        <name>EntsToRows</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE({!Select_Entitlements_To_Share},&quot;;&quot;,&quot;&lt;br/&gt;&quot;)</expression>
    </formulas>
    <formulas>
        <description>Ent ID&apos;s to Rows</description>
        <name>EntsToRowsID</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE({!Select_Entitlements_To_Share},&quot;;&quot;,&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <description>Grab the First Ent in the list</description>
        <name>FirstEnt</name>
        <dataType>String</dataType>
        <expression>LEFT({!ListOfSelectedID}, FIND(&quot;;&quot;, {!ListOfSelectedID})-1)</expression>
    </formulas>
    <formulas>
        <description>FirstWordOnly</description>
        <name>FirstWordOnly</name>
        <dataType>String</dataType>
        <expression>IF( FIND(&quot; &quot;, {!AccountName}) != 0,LEFT({!AccountName}, FIND(&quot; &quot;, {!AccountName}) ),{!AccountName})</expression>
    </formulas>
    <formulas>
        <description>Remove First Ent</description>
        <name>RemoveFirstEnt</name>
        <dataType>String</dataType>
        <expression>TRIM(SUBSTITUTE({!ListOfSelectedID},{!FirstEnt} + &quot;;&quot;,&quot;&quot;))</expression>
    </formulas>
    <formulas>
        <name>RestOfAccountName</name>
        <dataType>String</dataType>
        <expression>IF( FIND(&quot; &quot;, {!AccountName}) != 0,SUBSTITUTE({!AccountName},{!FirstWordOnly},&quot;&quot;),&quot;&quot;)</expression>
    </formulas>
