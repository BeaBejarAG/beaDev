        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
ISCHANGED([Case].Status),
ISPICKVAL(PRIORVALUE([Case].Status), &apos;Resolved&apos;) ,
NOT(ISPICKVAL([Case].Status, &apos;Closed&apos;))
)





</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_5</name>
        <dataType>Boolean</dataType>
        <expression>AND(
ISCHANGED({!myVariable_current.Status}),
ISPICKVAL(PRIORVALUE({!myVariable_current.Status}), &apos;Resolved&apos;) ,
NOT(ISPICKVAL({!myVariable_current.Status}, &apos;Closed&apos;))
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
ISCHANGED([Case].Status),
PRIORVALUE([Case].Status) == &quot;Resolved&quot;
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_7</name>
        <dataType>Boolean</dataType>
        <expression>AND(
ISCHANGED({!myVariable_current.Status}),
PRIORVALUE({!myVariable_current.Status}) == &quot;Resolved&quot;
)</expression>
