    <assignments>
        <name>Set_output_Deal_Type_Enterprise</name>
        <label>Set output Deal Type = Enterprise</label>
        <locationX>314</locationX>
        <locationY>398</locationY>
        <assignmentItems>
            <assignToReference>OppDealType</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>Enterprise</stringValue>
            </value>
        </assignmentItems>
    </assignments>
    <decisions>
        <description>If Opportunity Member is either in Compliance or Strategic Accounts, go back and flag the Opportunity Deal Type Accordingly.</description>
        <name>Role_is_Compliance_or_Enterprise</name>
        <label>Role is Compliance or Enterprise</label>
        <locationX>314</locationX>
        <locationY>278</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Compliance</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Get_Opp_Owner_Team_Member_Record.TeamMemberRole</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Compliance Team</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Opp_Owner_Team_Member_Record.User_Role_Name__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Compliance Team Member</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Opp_Owner_Team_Member_Record.User_Role_Name__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Head of Compliance</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_output_Deal_Type_Compliance</targetReference>
            </connector>
            <label>Compliance</label>
        </rules>
        <rules>
            <name>Strategic_Accounts</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Get_Opp_Owner_Team_Member_Record.TeamMemberRole</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Strategic Account Manager</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Opp_Owner_Team_Member_Record.User_Role_Name__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Enterprise</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_output_Deal_Type_Enterprise</targetReference>
            </connector>
            <label>Strategic Accounts</label>
        </rules>
    </decisions>
