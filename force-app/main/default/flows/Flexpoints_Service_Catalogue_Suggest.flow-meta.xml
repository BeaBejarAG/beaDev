    <formulas>
        <name>daysToDeliver</name>
        <dataType>Number</dataType>
        <expression>IF({!Get_Service_Catalogue_Item.Estimate_Required__c} = true,{!Days_to_Deliver},{!Get_Service_Catalogue_Item.Days_to_Deliver__c})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>hoursToDeliver</name>
        <dataType>Number</dataType>
        <expression>IF({!Get_Service_Catalogue_Item.Estimate_Required__c} = true,{!Hours_to_Deliver},{!Get_Service_Catalogue_Item.Hours_to_Deliver__c})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <description>Points Value, if estimated then uses that value</description>
        <name>PointsValue</name>
        <dataType>Number</dataType>
        <expression>IF({!Get_Service_Catalogue_Item.Estimate_Required__c} = true,CEILING(({!Service_Cost} * 0.63)/10)*10,{!Get_Service_Catalogue_Item.Points_Conversion__c})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>Service Cost if estimate calculate else get from Service Cost field on Services Cat item</description>
        <name>ServiceCostValue</name>
        <dataType>Number</dataType>
        <expression>IF({!Get_Service_Catalogue_Item.Estimate_Required__c} = true,{!Service_Cost},{!Get_Service_Catalogue_Item.Service_Cost__c})</expression>
        <scale>2</scale>
    </formulas>
