    <description>Case Close E-mail Alerts for all cases</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
[Case].RecordType.Name = &apos;Support&apos;,
ISPICKVAL([Case].RelatedProduct__c, &apos;Select HR&apos;),
ISCHANGED([Case].Status),
ISPICKVAL([Case].Status, &apos;Resolved&apos;),
ISBLANK([Case].No_Reply_3rd_Email_Sent__c),
NOT(ISBLANK([Case].ContactId)),
NOT(BEGINS([Case].Software_Version__c, &apos;4.&apos;))
) 
</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_6</name>
        <dataType>Boolean</dataType>
        <expression>AND(
{!myVariable_current.RecordType.Name} = &apos;Support&apos;,
ISPICKVAL({!myVariable_current.RelatedProduct__c}, &apos;Select HR&apos;),
ISCHANGED({!myVariable_current.Status}),
ISPICKVAL({!myVariable_current.Status}, &apos;Resolved&apos;),
