<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>48.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>CSP: My Cases</masterLabel>
    <targets>
        <target>lightningCommunity__Page</target>
        <target>lightningCommunity__Default</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightningCommunity__Default">
            <property name="componentTitle" type="String" default="My cases" label="Title"/>
            <property name="targetUrl" type="String" default="/cases" label="Target URL"/>
            <property name="displayStyle" type="String" label="Component Display Style" datasource="Horizontal,Vertical"/>
            <property name="ignoreProduct" type="Boolean" default="false" label="Ignore Product"/>
            <property name="superUser" type="Boolean" default="false" label="Allow super user access"/>
            <property name="HideAllAccountCases" type="Boolean" default="false" label="Hide All Accounts Box"/>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>