<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>48.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>CSP: Community Feeds</masterLabel>
    <targets>
        <target>lightningCommunity__Page</target>
        <target>lightningCommunity__Default</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightningCommunity__Default">
            <property name="recordId" type="String" default="" label="Record ID" />
            <property name="showHeader" type="Boolean" default="false" label="Show Header" />
            <property name="showUnanswered" type="Boolean" default="false" label="Show only unanswered" />
            <property name="hideCreateButton" type="Boolean" default="false" label="Hide create new post button" />
            <property name="cacheable" type="Boolean" default="false" label="Can cache this topic" />
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>